

<%= javascript_include_tag 'pagination' %>




<div id="tag_cloud">
  <% tag_cloud current_user.stacks.tag_counts, %w[s m l] do |tag, css_class| %>
    <%= link_to tag.name, tag_path(tag.name), class: css_class %>
  <% end %>


</div>

<div id = "stack-container"><%= render("stack-container")%></div>






